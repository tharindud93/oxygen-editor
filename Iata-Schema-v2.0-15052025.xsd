    <!-- ================= Track change and annotation element definition ============== -->
    
    <xs:element name="tc">
        <xs:annotation>
            <xs:documentation>Holding any new inserted/edited information during IAPP process</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:group ref="inline-element-group"/>
                <xs:element ref="tc"/>
                <xs:element ref="img"/>
                <xs:element ref="ol"/>
                <xs:element ref="ul"/>
                <xs:element ref="footnote"/>
                <xs:element ref="footnotes"/>
                <xs:element ref="address"/>
                <xs:element ref="notes"/>
                <xs:element ref="contact"/>
                <xs:element ref="index"/>
                <xs:element ref="input"/>
                <xs:element ref="annotation"/>
            </xs:choice>
            <xs:attribute ref="iapp-flag"/>
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
            <xs:attribute ref="style"/>
            
            <xs:assert test="not(img/@data-annotation-id or img/@data-annotation-seq or img/@data-ant or img/@data-user-role)"/>
            
        </xs:complexType>
    </xs:element>
    
    <xs:element name="annotation">
        <xs:annotation>
            <xs:documentation>Holding any new comment during IAPP process</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:element ref="img" minOccurs="0"/>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:group ref="inline-element-group"/>
                    <xs:element ref="tc"/>
                    <xs:element ref="annotation"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute ref="iapp-flag"/>
            <xs:attributeGroup ref="annotation-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
        </xs:complexType>
    </xs:element>
    
    
    
