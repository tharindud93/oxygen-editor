    <!-- ============== Section definition ======================= -->
    
    <xs:element name="section">
        <xs:annotation>
            <xs:documentation>Use as a root element of the pulication along with different level of sections as well</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="number" minOccurs="0"/>
                <xs:element ref="heading"/>
                <xs:element ref="subheading" minOccurs="0"/>
                <xs:element ref="index" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="rule-info" minOccurs="0"/>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="address"/>
                    <xs:element ref="figure"/>
                    <xs:element ref="numbered-para"/>
                    <xs:element ref="ol"/>
                    <xs:element ref="ul"/>
                    <xs:element ref="img"/>
                    <xs:element ref="notes"/>
                    <xs:element ref="p"/>
                    <xs:element ref="table"/>
                    <xs:element ref="table-group"/>
                    <xs:element ref="definition-group"/>
                    <xs:element ref="definition"/>
                    <xs:element ref="example"/>
                    <xs:element ref="examples"/>
                    <xs:element ref="informal-section"/>
                    <xs:element ref="steps"/>
                    <xs:element ref="extracts"/>
                    <xs:element ref="airimp"/>
                    <xs:element ref="codes"/>
                    <xs:element ref="metadata"/>
                    <xs:element ref="guidance"/>
                    <xs:element ref="state"/>
                    <xs:element ref="operator"/>
                    <xs:element ref="var-ref-set"/><!-- maxOccur need to set 2 in Schematron if role value is 'General-Requirements|Container-Requirement' or parent is packing-instruction  --> 
                    <xs:element ref="animal-set"/><!-- maxOccur need to set 1 in Schematron if role value is 'Container-Requirement'-->
                    <xs:element ref="publication"/>
                    
                    
                    
                    
                    <!--<xs:element ref="key-learning-point"/>-->
                    <!--<xs:element ref="did-you-know"/>
                    <xs:element ref="progress-check"/>
                    <xs:element ref="try-activity"/>-->
                    <!--<xs:element ref="interview"/>-->
                    
                    <!--<xs:element ref="active-link"/>-->
                    
                </xs:choice>
                <!--<xs:element ref="section" minOccurs="0" maxOccurs="unbounded"/>-->
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="active-link"/>
                    <xs:element ref="section"/>
                    <xs:element ref="packing-instructions"/>
                </xs:choice>
                <xs:element ref="footnotes" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute ref="id"/>
            <xs:attribute ref="legacy-id"/>
            <xs:attribute ref="publication-segment"/>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="page-wide"/>
            <xs:attribute ref="auto-seq"/>
            <xs:attribute ref="auto-seq-format">
                <xs:annotation>
                    <xs:appinfo>
                        <readOnly>true</readOnly>
                    </xs:appinfo>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="auto-seq-start" type="xs:string"/>
            <xs:attribute ref="role"/>
            <xs:attribute ref="level"/>
            <xs:attribute ref="style"/>
            <xs:attribute name="title-running-header" type="xs:string"/>
            <xs:attribute name="title-running-footer" type="xs:string"/>
            <xs:attribute ref="icon"/>
            <xs:attribute ref="epub-or-print"/>
            <xs:attribute ref="xml:lang"/>
            <xs:attribute ref="effective"/>
            <xs:attribute ref="iapp-flag"/>
            <xs:attribute ref="orient"/>
            <!--<xs:attribute name="one-column" type="yes-no-att"/>-->
            <xs:attribute ref="bgcolor"/>
            
            <xs:attribute ref="auditable"/>
            <xs:attribute ref="next-section-checklist"/>
            <xs:attribute name="permid" type="xs:string"/>
            
            <xs:attribute name="portal-section" type="xs:string"/>
            <xs:attribute name="portal-requirement" type="xs:string"/>
            <xs:attribute name="gap-not-required" type="xs:string"/>
            <xs:attribute name="gap-disabled" type="xs:string"/>
            
            <xs:attribute name="thumb-tab" type="yes-no-att"/>
            
            
            
            <xs:attribute name="Aircraft-Type-Code" type="xs:string"/>
            
            <!--<xs:attribute name="placeholder" type="yes-no-att"/>-->
            
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
            
            <xs:assert test="not(img/@data-annotation-id or img/@data-annotation-seq or img/@data-ant or img/@data-user-role)"/>
        </xs:complexType>
    </xs:element>
    
    <!-- ===================================================================================== -->
    
    
    <!-- ===================== Container group elements definition ============================ -->
    
    
    <xs:element name="ol">
        <xs:annotation>
            <xs:documentation>To define numbered list. Example:-
                <ol type="numerical">
                    <li><p>list item 1</p></li>
                    <li><p>list item 2</p></li>
                </ol>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence maxOccurs="unbounded">
                <xs:element ref="li"/>
            </xs:sequence>
            <xs:attribute name="type" type="list-type"/>
            <xs:attribute name="start" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Use to start numbered list with specific number</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="style"/>
            <xs:attribute ref="effective"/>
            <xs:attribute ref="icon"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="ul">
        <xs:annotation>
            <xs:documentation>To define unnumbered list. Example:-
                <ul type="bullet">
                    <li><p>list item 1</p></li>
                    <li><p>list item 2</p></li>
                </ul></xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence maxOccurs="unbounded">
                <xs:element ref="li"/>
            </xs:sequence>
            <xs:attribute name="type" type="list-type"/>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="style"/>
            <xs:attribute ref="icon"/>
            <xs:attribute ref="effective"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="li">
        <xs:annotation>
            <xs:documentation>To define list item. Example:-
                <ol type="numerical">
                    <li><p>list item 1</p></li>
                    <li><p>list item 2</p></li>
                </ol></xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="p"/>
                <xs:element ref="address"/>
                <xs:element ref="notes"/>
                <xs:element ref="figure"/>
                <xs:element ref="table"/>
                <xs:element ref="table-group"/>
                <xs:element ref="img"/>
                <xs:element ref="ol"/>
                <xs:element ref="ul"/>
                <xs:element ref="definition"/>
                <xs:element ref="definition-group"/>
                <xs:element ref="example"/>
                <xs:element ref="examples"/>
                <xs:element ref="airimp"/>
                <xs:element ref="codes"/>
                <xs:element ref="active-link"/>
            </xs:choice>
            <!--<xs:attribute ref="id"/>
            <xs:attribute ref="legacy-id"/>-->
            <xs:attribute ref="style"/>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="icon"/>
            <xs:attribute ref="epub-or-print"/>
            <xs:attribute ref="effective"/>
            <xs:attribute ref="iapp-flag"/>
            <xs:attribute ref="auditable"/>
            
            <xs:attribute name="gap-disabled" type="xs:string"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
            
            <xs:assert test="not(img/@data-annotation-id or img/@data-annotation-seq or img/@data-ant or img/@data-user-role)"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="address">
        <xs:annotation>
            <xs:documentation>To capture information of address. Example:-
                <address>
                    <p>BIC (Bureau International des Containers)</p>
                    <p>14, rue Jean-Roy</p>
                    <p>F-75015 </p>
                    <p>Paris (France)</p>
                    <contact>
                    <phone>+49 (0) 7229 1868 163</phone>
                    <fax>Fax: +49 (0) 7229 1868 165</fax>
                    <link-to href="mailto:g.hasel@logar.de">g.hasel@logar.de</link-to>
                </contact>
                </address>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <!--<xs:element ref="org-name" minOccurs="0"/>
                <xs:element ref="free-text-address" minOccurs="0"/>
                <xs:element ref="p" minOccurs="0"/>-->
                <xs:element ref="p" minOccurs="0"/>
                <xs:element ref="party-info" minOccurs="0"/>
                <xs:element ref="address-details" minOccurs="0"/>
                <xs:element ref="contact" minOccurs="0"/>
            </xs:sequence>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="style"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="party-info">
        <xs:annotation>
            <xs:documentation></xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="p" maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="style"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="address-details">
        <xs:annotation>
            <xs:documentation></xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="p" maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="style"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
        </xs:complexType>
    </xs:element>
    
    
    <!--<xs:element name="org-name">
        <xs:annotation>
            <xs:documentation></xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="p" maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
            
        </xs:complexType>
    </xs:element>
    
    <xs:element name="free-text-address">
        <xs:annotation>
            <xs:documentation></xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="p" maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
            
        </xs:complexType>
    </xs:element>-->
    
    
    <xs:element name="contact">
        <xs:annotation>
            <xs:documentation>To capture contact information. Example:-
                <contact>
                    <phone>+49 (0) 7229 1868 163</phone>
                    <fax>Fax: +49 (0) 7229 1868 165</fax>
                    <link-to href="mailto:g.hasel@logar.de">g.hasel@logar.de</link-to>
                </contact>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="phone"/>
                <xs:element ref="fax"/>
                <xs:element ref="link-to"/>
                <xs:element ref="cable"/>
                <xs:element ref="telex"/>
            </xs:choice>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="style"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
        </xs:complexType>
    </xs:element>

    
    <xs:element name="notes">
        <xs:annotation>
            <xs:documentation>Wrapper of single or multiple note element: Example:-
            <notes>
                <note><p>note 1</p></note>
                <note><p>note 2</p></note>
            </notes>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="note"/>
                <xs:element ref="active-link"/>
            </xs:choice>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="style"/>
            <xs:attribute ref="effective"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="note">
        <xs:annotation>
            <xs:documentation>To capture specific note related information. Example:-
                <notes>
                    <note><p>note 1</p></note>
                    <note><p>note 2</p></note>
                </notes>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="figure"/>
                <xs:element ref="img"/>
                <xs:element ref="p"/>
                <xs:element ref="ol"/>
                <xs:element ref="ul"/>
                <xs:element ref="table"/>
                <xs:element ref="table-group"/>
                <!--<xs:element ref="active-link"/>-->
                </xs:choice>
            <xs:attribute ref="id"/>
            <xs:attribute ref="legacy-id"/>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="style"/>
            <xs:attribute name="type" type="note-type"/>
            <xs:attribute ref="icon"/>
            <xs:attribute ref="epub-or-print"/>
            <xs:attribute ref="effective"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attribute ref="auditable"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
            
            <xs:assert test="not(img/@data-annotation-id or img/@data-annotation-seq or img/@data-ant or img/@data-user-role)"/>
        </xs:complexType>
    </xs:element>
    
    
    <xs:element name="numbered-para">
        <xs:annotation>
            <xs:documentation>Paragraph that is numbered sequentially. Example:- 
                <numbered-para>
                <number>2.1.1</number>
                <p>The regulations given in this section shall be observed for the
                    transportation of automobiles on wide-body aircraft.</p>
            </numbered-para>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="number"/>
                <xs:choice maxOccurs="unbounded">
                    <xs:element ref="ol"/>
                    <xs:element ref="ul"/>
                    <xs:element ref="p"/>
                    <xs:element ref="example"/>
                    <xs:element ref="examples"/>
                    <xs:element ref="definition-group"/>
                    <xs:element ref="definition"/>
                    <xs:element ref="notes"/>
                    <xs:element ref="table"/>
                    <xs:element ref="table-group"/>
                    <xs:element ref="metadata"/>
                    <xs:element ref="guidance"/>
                    <xs:element ref="ref-group"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute ref="id"/>
            <xs:attribute ref="legacy-id"/>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="style"/>
            <xs:attribute ref="icon"/>
            <xs:attribute ref="epub-or-print"/>
            <xs:attribute ref="effective"/>
            
            <xs:attribute name="remote-audit" type="xs:string"/><!-- under client query -->
            <!--<xs:attribute name="placeholder" type="yes-no-att"/>--><!-- under client query -->
            
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="figure">
        <xs:annotation>
            <xs:documentation>Container for a graphical object with its
                surrounding text like Figure number, heading, notes, source....etc. Example:-
                <figure id="fig1">
                    <number>1</number>
                    <heading>AREA LOAD</heading>
                        <img src="uld-en-ahm_513_2.3aread.jpg" style="lsrc:uld-en-ahm_513_2.3aread.jpg;hsrc:uld-en-ahm_513_2.3aread.eps;"/>
                </figure>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="number" minOccurs="0"/>
                <xs:element ref="heading" minOccurs="0"/>
                <xs:choice maxOccurs="unbounded">
                    <xs:element ref="img" minOccurs="1"/>
                    <xs:element ref="notes" minOccurs="0"/>
                    <xs:element ref="p" minOccurs="0"/>
                    <xs:element ref="active-link" minOccurs="0"/>
                </xs:choice>
                <xs:element ref="source" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute ref="id"/>
            <xs:attribute ref="legacy-id"/>
            <xs:attribute ref="page-wide"/>
            <xs:attribute ref="orient"/>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="style"/>
            <xs:attribute ref="icon"/>
            <xs:attribute ref="epub-or-print"/>
            <xs:attribute ref="auto-seq"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
            
            <xs:assert test="not(img/@data-annotation-id or img/@data-annotation-seq or img/@data-ant or img/@data-user-role)"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="index">
        <xs:annotation>
            <xs:documentation>This element is used to capture index information. Example:- 
                <index>
                <primary-key>Regulatory Overview</primary-key>
                <secondary-key>Multi-modal</secondary-key>
            </index>
                The contents of the Index element (primary-key and
                secondary-key) can be used by output applications when creating data for indexing
                purposes.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="primary-key"/>
                <xs:element ref="secondary-key" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
            <!--<xs:attribute ref="id"/>
            <xs:attribute ref="legacy-id"/>-->
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="style"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
        </xs:complexType>
    </xs:element>
    
    
    <xs:element name="table-group">
        <xs:annotation>
            <xs:documentation>Container for a Table with its related features, like
                TableHeading, TableNumber, TableFootNotes, and others. Example:-
            <table-group>
                <number>1</number>
                <heading>Table example</heading>
                <table frame="box" width="100%">
                    <colgroup>
                        <col width="50%"/>
                        <col width="50%"/>
                    </colgroup>
                    <thead>
                        <tr>
                            <th align="center" valign="middle"><p>Base size code</p></th>
                            <th align="center" valign="middle"><p>Base dimensions</p></th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td align="center" valign="middle"><p><b>A</b></p></td>
                            <td align="center" valign="middle"><p>2 235 x 3 175</p></td>
                        </tr>
                    </tbody>
                </table>
                <table-footnotes>
                    <table-footnote callout="a" id="DGR-2.7.A-a"><p>The more restrictive quantity
                        required by either the primary risk or where a Division 6.1 subsidiary risk
                        label is required.</p></table-footnote>
                    <table-footnote callout="b" id="DGR-2.7.A-b"><p>For inner packagings, the
                        quantity contained in receptacle with a water capacity of 30mL. For outer
                        packagings, the sum of the water capacities of all the inner packagings
                        contained must not exceed 1 L.</p></table-footnote>
                </table-footnotes>
                <source>This is source information.</source>
            </table-group>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="number" minOccurs="0"/>
                <xs:element ref="heading"/>
                <xs:choice maxOccurs="unbounded">
                    <xs:element ref="table"/>
                    <xs:element ref="notes" minOccurs="0"/>
                    <xs:element ref="p" minOccurs="0"/>
                    <xs:element ref="active-link" minOccurs="0"/>
                </xs:choice>
                <xs:element ref="table-footnotes" minOccurs="0"/>
                <xs:element ref="source" minOccurs="0"/>
            </xs:sequence>
            <xs:attribute ref="id"/>
            <xs:attribute ref="legacy-id"/>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="style"/>
            <xs:attribute ref="page-wide"/>
            <xs:attribute ref="auto-seq"/>
            <xs:attribute ref="icon"/>
            <xs:attribute ref="epub-or-print"/>
            
            <xs:attribute name="permid" type="xs:string"/>
            <xs:attribute name="portal-section" type="xs:string"/>
            <xs:attribute name="portal-requirement" type="xs:string"/>
                
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="table">
        <xs:annotation>
            <xs:documentation>To capture table - derived from html table standard. Example:-
                <table frame="box" width="100%">
                    <colgroup>
                        <col width="50%"/>
                        <col width="50%"/>
                    </colgroup>
                    <thead>
                        <tr>
                            <th align="center" valign="middle"><p>Base size code</p></th>
                            <th align="center" valign="middle"><p>Base dimensions</p></th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td align="center" valign="middle"><p><b>A</b></p></td>
                            <td align="center" valign="middle"><p>2 235 x 3 175</p></td>
                        </tr>
                    </tbody>
                </table>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="caption" minOccurs="0"/>
                <xs:element ref="colgroup" minOccurs="0"/>
                <xs:element ref="thead" minOccurs="0"/>
                <xs:element ref="tbody"/>
                <!--<xs:element ref="source" minOccurs="0"/>-->
            </xs:sequence>
            <xs:attribute ref="id"/>
            <xs:attribute ref="legacy-id"/>
            <xs:attribute ref="style"/>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute name="width" type="multi-length"/>
            <xs:attribute name="border" type="Pixels"/>
            <xs:attribute name="frame" type="TFrame"/>
            <xs:attribute name="rules" type="TRules"/>
            <xs:attribute name="cellspacing" type="Length"/>
            <xs:attribute name="cellpadding" type="Length"/>
            <xs:attribute ref="tblheader"/>
            <xs:attribute ref="orient"/>
            <xs:attribute ref="bgcolor"/>
            <xs:attribute name="display-on-one-page" type="true-false-att"/>
            
            <xs:attribute ref="page-wide"/>
            
            <xs:attribute name="permid" type="xs:string"/>
            <xs:attribute name="portal-section" type="xs:string"/>
            <xs:attribute name="portal-requirement" type="xs:string"/>
            
            <!--<xs:attribute ref="iapp-style"/>-->
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
        </xs:complexType>
    </xs:element>
    
    
    <xs:element name="colgroup">
        <xs:annotation>
            <xs:documentation>colgroup groups a set of col elements. It allows you to group several
                semantically related columns together.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="col" maxOccurs="unbounded"/>
            </xs:sequence>
            <!--<xs:attribute ref="id"/>
            <xs:attribute ref="legacy-id"/>-->
            <xs:attribute name="width" type="multi-length"/>
            <xs:attribute ref="align"/>
            <xs:attribute ref="valign"/>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="style"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="thead">
        <xs:annotation>
            <xs:documentation>Root element to capture table header information</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="tr" maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="style"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="tbody">
        <xs:annotation>
            <xs:documentation>Use tbody to capture table body information</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="tr" maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="tr">
        <xs:annotation>
            <xs:documentation>Use tr to capture table row information</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="th"/>
                <xs:element ref="td"/>
            </xs:choice>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="style"/>
            <!--<xs:attribute ref="iapp-style"/>-->
            <xs:attribute ref="iapp-flag"/>
            <xs:attribute name="draggable" type="true-false-att"/>
            
            
            <xs:attribute name="toc" type="yes-no-att"/>
            
            <xs:attribute name="permid" type="xs:string"/>
            <xs:attribute name="portal-section" type="xs:string"/>
            <xs:attribute name="portal-requirement" type="xs:string"/>
            
            <xs:attribute name="gap-disabled" type="xs:string"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="th">
        <xs:annotation>
            <xs:documentation>Use th to capture table header information inside thead</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="p"/>
                <xs:element ref="notes"/>
                <xs:element ref="ol"/>
                <xs:element ref="ul"/>
                <xs:element ref="figure"/>
                <xs:element ref="img"/>
                <xs:element ref="address"/>
                <xs:element ref="definition"/>
                <xs:element ref="example"/>
                <xs:element ref="examples"/>
                <xs:element ref="steps"/>
                <xs:element ref="active-link"/>
            </xs:choice>
            <!--<xs:attribute ref="id"/>
            <xs:attribute ref="legacy-id"/>-->
            <xs:attribute ref="style"/>
            <xs:attribute ref="align"/>
            <xs:attribute ref="valign"/>
            <xs:attribute name="rowspan" type="Number"/>
            <xs:attribute name="colspan" type="Number"/>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="scope"/>
            <!--<xs:attribute ref="iapp-style"/>-->
            <xs:attribute ref="dir"/>
            <xs:attribute ref="effective"/>
            <xs:attribute ref="bgcolor"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attribute name="permid" type="xs:string"/>
            <xs:attribute name="portal-section" type="xs:string"/>
            <xs:attribute name="portal-requirement" type="xs:string"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
            
            <xs:assert test="not(img/@data-annotation-id or img/@data-annotation-seq or img/@data-ant or img/@data-user-role)"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="td">
        <xs:annotation>
            <xs:documentation>Use td to capture cell data within table body</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="p"/>
                <xs:element ref="notes"/>
                <xs:element ref="ol"/>
                <xs:element ref="ul"/>
                <xs:element ref="figure"/>
                <xs:element ref="img"/>
                <xs:element ref="address"/>
                <xs:element ref="definition"/>
                <xs:element ref="example"/>
                <xs:element ref="examples"/>
                <xs:element ref="steps"/>
                <xs:element ref="active-link"/>
            </xs:choice>
            <!--<xs:attribute ref="id"/>
            <xs:attribute ref="legacy-id"/>-->
            <xs:attribute ref="style"/>
            <xs:attribute ref="align"/>
            <xs:attribute ref="valign"/>
            <xs:attribute name="rowspan" type="Number"/>
            <xs:attribute name="colspan" type="Number"/>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="scope"/>
            <!--<xs:attribute ref="iapp-style"/>-->
            <xs:attribute ref="dir"/>
            <xs:attribute ref="effective"/>
            <xs:attribute ref="bgcolor"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attribute name="permid" type="xs:string"/>
            <xs:attribute name="portal-section" type="xs:string"/>
            <xs:attribute name="portal-requirement" type="xs:string"/>
            
            <xs:attribute ref="auditable"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
            
            <xs:assert test="not(img/@data-annotation-id or img/@data-annotation-seq or img/@data-ant or img/@data-user-role)"/>
        </xs:complexType>
    </xs:element>
    
    
    <xs:element name="table-footnotes">
        <xs:annotation>
            <xs:documentation>Container for all table footnote, even when there is
                only one. Example:- <table-footnotes>
                    <table-footnote callout="a" id="DGR-2.7.A-a"><p>The more restrictive quantity
                        required by either the primary risk or where a Division 6.1 subsidiary risk
                        label is required.</p></table-footnote>
                    <table-footnote callout="b" id="DGR-2.7.A-b"><p>For inner packagings, the
                        quantity contained in receptacle with a water capacity of 30mL. For outer
                        packagings, the sum of the water capacities of all the inner packagings
                        contained must not exceed 1 L.</p></table-footnote>
                </table-footnotes></xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="table-footnote" maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="table-footnote">
        <xs:annotation>
            <xs:documentation>Container for a table footnote, rendered after the
                current Table in printed and electronic versions. Example:- <table-footnotes>
                    <table-footnote callout="a" id="DGR-2.7.A-a"><p>The more restrictive quantity
                        required by either the primary risk or where a Division 6.1 subsidiary risk
                        label is required.</p></table-footnote>
                    <table-footnote callout="b" id="DGR-2.7.A-b"><p>For inner packagings, the
                        quantity contained in receptacle with a water capacity of 30mL. For outer
                        packagings, the sum of the water capacities of all the inner packagings
                        contained must not exceed 1 L.</p></table-footnote>
                </table-footnotes></xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="p" minOccurs="0"/>
                <xs:element ref="notes" minOccurs="0"/>
                <xs:element ref="img" minOccurs="0"/>
                <xs:element ref="ol" minOccurs="0"/>
                <xs:element ref="ul" minOccurs="0"/>
                <xs:element ref="active-link" minOccurs="0"/>
            </xs:choice>
            <xs:attribute ref="id"/>
            <xs:attribute ref="legacy-id"/>
            <xs:attribute ref="callout"/>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="style"/>
            <xs:attribute ref="icon"/>
            <xs:attribute ref="epub-or-print"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
            
            <xs:assert test="not(img/@data-annotation-id or img/@data-annotation-seq or img/@data-ant or img/@data-user-role)"/>
        </xs:complexType>
    </xs:element>
    
    
    <xs:element name="footnotes">
        <xs:annotation>
            <xs:documentation>Container for all footnotes, even when there is
                only one. Example:- <footnotes>
                    <footnote id="footnote-1" callout="1"><p>This is first footnote</p></footnote>
                    <footnote id="footnote-2" callout="2"><p>This is second footnote</p></footnote>
            </footnotes></xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="footnote" maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="footnote">
        <xs:annotation>
            <xs:documentation>Container for a footnote, usually rendered at the
                bottom of the current page in printed versions or at the end of the file for
                electronic version. Example:- <footnotes>
                    <footnote id="footnote-1" callout="1"><p>This is first footnote</p></footnote>
                    <footnote id="footnote-2" callout="2"><p>This is second footnote</p></footnote>
                </footnotes></xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="p"/>
                <xs:element ref="ol"/>
                <xs:element ref="ul"/>
                <xs:element ref="notes"/>
                <xs:element ref="active-link"/>
            </xs:choice>
            <xs:attribute ref="id"/>
            <xs:attribute ref="legacy-id"/>
            <xs:attribute ref="callout"/>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="style"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="meaning">
        <xs:annotation>
            <xs:documentation>This element is usually associated with the term
                element and describes it. Example:- <meaning><p>may contain explosives (flares or other pyrotechnics),
                    chemical oxygen generators, unserviceable tyre assemblies, cylinders of
                    compressed gas (oxygen, carbon dioxide, nitrogen or fire extinguishers), paint,
                    adhesives, aerosols, life-saving appliances, first aid kits, fuel in equipment,
                    wet or lithium batteries, matches, etc.</p></meaning>
                    
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="p"/>
                <xs:element ref="table"/>
                <xs:element ref="table-group"/>
                <xs:element ref="notes"/>
                <xs:element ref="numbered-para"/>
                <xs:element ref="img"/>
                <xs:element ref="figure"/>
                <xs:element ref="ol"/>
                <xs:element ref="ul"/>
                <xs:element ref="example"/>
                <xs:element ref="examples"/>
                <!--<xs:element ref="see"/>
                <xs:element ref="see-also"/>--><!-- based on client, removed it -->
                <xs:element ref="definition-group"/>
                <xs:element ref="definition"/>
                <xs:element ref="active-link"/>
            </xs:choice>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="style"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
            
            <xs:assert test="not(img/@data-annotation-id or img/@data-annotation-seq or img/@data-ant or img/@data-user-role)"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="definition">
        <xs:annotation>
            <xs:documentation>Container for a term and it's meaning. Example:- <definition>
                <term separator="mdash">AIRCRAFT SPARE PARTS/AIRCRAFT EQUIPMENT</term>
                <meaning><p>may contain explosives (flares or other pyrotechnics), chemical oxygen
                    generators, unserviceable tyre assemblies, cylinders of compressed gas
                    (oxygen, carbon dioxide, nitrogen or fire extinguishers), paint, adhesives,
                    aerosols, life-saving appliances, first aid kits, fuel in equipment, wet or
                    lithium batteries, matches, etc.</p></meaning>
            </definition>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="term"/>
                <xs:element ref="meaning"/>
                <!--<xs:choice maxOccurs="unbounded">
                    <xs:element ref="meaning"/>
                    <xs:element ref="equivalent-term" minOccurs="0"/>
                </xs:choice>-->
            </xs:sequence>
            <xs:attribute ref="id"/>
            <xs:attribute ref="legacy-id"/>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="style"/>
            <xs:attribute ref="icon"/>
            <xs:attribute ref="epub-or-print"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="definition-group">
        <xs:annotation>
            <xs:documentation>Container for a group of Definitions and have an
                optional heading. Example:- 
                <definition-group>
                    <heading>Defintion Group</heading>
                    <definition>
                        <term separator="mdash">AIRCRAFT SPARE PARTS/AIRCRAFT
                            EQUIPMENT</term>
                        <meaning><p>may contain explosives (flares or other pyrotechnics), chemical
                            oxygen generators, unserviceable tyre assemblies, cylinders of
                            compressed gas (oxygen, carbon dioxide, nitrogen or fire extinguishers),
                            paint, adhesives, aerosols, life-saving appliances, first aid kits, fuel
                            in equipment, wet or lithium batteries, matches, etc.</p></meaning>
                    </definition>
                    <definition>
                        <term separator="mdash">AUTOMOBILES, AUTOMOBILE PARTS</term>
                        <meaning><p>(car, motor, motorcycle) may contain ferro-magnetic material which
                            may not meet the definition for magnetized material but which may be
                            subject to special stowage requirements due to the possibility of
                            affecting aircraft instruments (see <a href="DGR-3.9.1.2"
                                >3.9.2.2</a>). May also contain engines, carburettors or fuel
                            tanks which contain or have contained fuel, wet batteries, compressed
                            gases in tyre inflation devices, fire extinguishers, shocks/struts with
                            nitrogen, air bag inflators/air bag modules, etc.</p></meaning>
                    </definition>
                </definition-group>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="heading"/>
                <xs:element ref="definition" maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:attribute ref="id"/>
            <xs:attribute ref="legacy-id"/>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="style"/>
            <xs:attribute ref="icon"/>
            <xs:attribute ref="epub-or-print"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="examples">
        <xs:annotation>
            <xs:documentation></xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="heading"/>
                <xs:element ref="example" maxOccurs="unbounded"/>
                <!--<xs:choice maxOccurs="unbounded">
                    <xs:element ref="p" minOccurs="0"/>
                    <xs:element ref="definition" minOccurs="0"/>
                    <xs:element ref="figure" minOccurs="0"/>
                    <xs:element ref="img" minOccurs="0"/>
                    <xs:element ref="ol" minOccurs="0"/>
                    <xs:element ref="ul" minOccurs="0"/>
                    <xs:element ref="notes" minOccurs="0"/>
                    <xs:element ref="table" minOccurs="0"/>
                    <xs:element ref="table-group" minOccurs="0"/>
                    <xs:element ref="steps" minOccurs="0"/>
                    <xs:element ref="airimp" minOccurs="0"/>
                    <xs:element ref="example" minOccurs="0"/>
                    <xs:element ref="active-link" minOccurs="0"/>
                </xs:choice>-->
                
            </xs:sequence>
            <xs:attribute ref="id"/>
            <xs:attribute ref="legacy-id"/>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="style"/>
            <xs:attribute ref="icon"/>
            <xs:attribute ref="epub-or-print"/>
            <xs:attribute ref="effective"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
            
        </xs:complexType>
    </xs:element>
    
    <xs:element name="example">
        <xs:annotation>
            <xs:documentation>Content model to capture examples. Example:-
              <example>
                <heading>Examples:</heading>
                <p>
                    <img src="ahm-e-011-7page"/>
                </p>
            </example>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="heading"/>
                <xs:choice maxOccurs="unbounded">
                    <xs:element ref="p" minOccurs="0"/>
                    <xs:element ref="definition" minOccurs="0"/>
                    <xs:element ref="figure" minOccurs="0"/>
                    <xs:element ref="img" minOccurs="0"/>
                    <xs:element ref="ol" minOccurs="0"/>
                    <xs:element ref="ul" minOccurs="0"/>
                    <xs:element ref="notes" minOccurs="0"/>
                    <xs:element ref="table" minOccurs="0"/>
                    <xs:element ref="table-group" minOccurs="0"/>
                    <xs:element ref="steps" minOccurs="0"/>
                    <xs:element ref="airimp" minOccurs="0"/>
                    <xs:element ref="active-link" minOccurs="0"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute ref="id"/>
            <xs:attribute ref="legacy-id"/>
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="style"/>
            <xs:attribute ref="icon"/>
            <xs:attribute ref="epub-or-print"/>
            <xs:attribute ref="effective"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
            
            <xs:assert test="not(img/@data-annotation-id or img/@data-annotation-seq or img/@data-ant or img/@data-user-role)"/>
        </xs:complexType>
    </xs:element>
    
    
    <xs:element name="informal-section">
        <xs:annotation>
            <xs:documentation>Use to capture informal section</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="label"/>
                <xs:choice maxOccurs="unbounded">
                    <xs:element ref="definition" minOccurs="0"/>
                    <xs:element ref="example" minOccurs="0"/>
                    <xs:element ref="examples"/>
                    <xs:element ref="figure" minOccurs="0"/>
                    <xs:element ref="img" minOccurs="0"/>
                    <xs:element ref="ol" minOccurs="0"/>
                    <xs:element ref="ul" minOccurs="0"/>
                    <xs:element ref="notes" minOccurs="0"/>
                    <xs:element ref="p" minOccurs="0"/>
                    <xs:element ref="table" minOccurs="0"/>
                    <xs:element ref="table-group" minOccurs="0"/>
                    <xs:element ref="address" minOccurs="0"/>
                    <xs:element ref="steps" minOccurs="0"/>
                    <xs:element ref="airimp" minOccurs="0"/>
                    <xs:element ref="active-link" minOccurs="0"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute ref="id"/>
            <xs:attribute ref="legacy-id"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
            
            <xs:assert test="not(img/@data-annotation-id or img/@data-annotation-seq or img/@data-ant or img/@data-user-role)"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="steps">
        <xs:annotation>
            <xs:documentation>container of single or multiple step structure</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="heading" minOccurs="0"/>
                <xs:element ref="step" maxOccurs="unbounded"/>
            </xs:sequence>
            <!--<xs:attribute ref="id"/>
            <xs:attribute ref="legacy-id"/>-->
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="step">
        <xs:annotation>
            <xs:documentation>This construct is designed to encapsulate procedure
                type content presented as a sequence of steps. These steps are of a 'floating'
                nature and not rigidly procedural. Example:- 
                <steps>
                <step>
                    <heading>Step 1.</heading>
                    <p>UN number or ID number (from Column A) preceded by the prefix ...</p>
                </step>
                </steps>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="heading"/>
                <xs:choice maxOccurs="unbounded">
                    <xs:element ref="definition" minOccurs="0"/>
                    <xs:element ref="example" minOccurs="0"/>
                    <xs:element ref="examples"/>
                    <xs:element ref="figure" minOccurs="0"/>
                    <xs:element ref="img" minOccurs="0"/>
                    <xs:element ref="ol" minOccurs="0"/>
                    <xs:element ref="ul" minOccurs="0"/>
                    <xs:element ref="notes" minOccurs="0"/>
                    <xs:element ref="p" minOccurs="0"/>
                    <xs:element ref="table-group" minOccurs="0"/>
                    <xs:element ref="table" minOccurs="0"/>
                    <xs:element ref="airimp" minOccurs="0"/>
                    <xs:element ref="active-link" minOccurs="0"/>
                </xs:choice>
            </xs:sequence>
            <!--<xs:attribute ref="id"/>
            <xs:attribute ref="legacy-id"/>-->
            <xs:attributeGroup ref="rev-group"/>
            <xs:attribute ref="style"/>
            <xs:attribute ref="icon"/>
            <xs:attribute ref="epub-or-print"/>
            <xs:attribute ref="iapp-flag"/>
            
            <xs:attributeGroup ref="track-changes-attributes-m"/>
            <xs:attribute ref="inno_ref"/>
            
            <xs:assert test="not(img/@data-annotation-id or img/@data-annotation-seq or img/@data-ant or img/@data-user-role)"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="extracts">
        <xs:annotation>
            <xs:documentation>Description:- This element is used for contents from other
                publication. It allows typesetting and electronic publication to identify such
                extracts and format them appropriately.
                Example:-
                <extracts>
                    <source>Universal Postal Convention, paragraph 6</source>
                    <para>6 - The insertion of live animals in letter-post items shall be
                        prohibited.</para>
                    <para>6.1 - However, the following shall be admitted in letter-post items
                        ...</para>
                    <para>6.1.1 - bees, leeches and silk-worms;</para>
                    <para>6.1.2 - parasites and destroyers of noxious insects intended for
                        ...</para>
                </extracts>
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="source" minOccurs="0"/>
                <xs:choice>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="p" minOccurs="0"/>
                    <xs:element ref="ol" minOccurs="0"/>
                    <xs:element ref="ul" minOccurs="0"/>
                    <xs:element ref="definition" minOccurs="0"/>
                    <xs:element ref="definition-group" minOccurs="0"/>
                    <xs:element ref="example" minOccurs="0"/>
                    <xs:element ref="examples" minOccurs="0"/>
                    <xs:element ref="figure" minOccurs="0"/>
